# Makefile.in for Paradise server src/common 3.1
# @configure_input@

@SET_MAKE@

bindir=@bindir@
datadir=@datadir@
exec_prefix=@exec_prefix@
libdir=@libdir@
libexecdir=@libexecdir@
mandir=@mandir@
prefix=@prefix@
srcdir=@srcdir@
top_srcdir=@top_srcdir@

AR=ar
ARFLAGS=cruv
RANLIB=@RANLIB@

CC=@CC@
CPP=@CPP@

CFLAGS=@CFLAGS@
CPPFLAGS=@CPPFLAGS@ -I../include
DEFS=@AUTH_DEFS@ @DEFS@
LDFLAGS=@LDFLAGS@
LIBS=@LIBS@ -lm

INSTALL=@INSTALL@
INSTALL_SCRIPT=@INSTALL_SCRIPT@
INSTALL_DATA=@INSTALL_DATA@

VPATH=../common:../rsa

####

ROBOT_OBJS=robotII.o rmove.o

all: robotII

robotII: common.a robotII.o $(ROBOT_OBJS)
	$(CC) $(CFLAGS) -o robotII $(ROBOT_OBJS) ../common/common.a ../common/robot.a $(LDFLAGS) $(LIBS)

clean:
	-rm -f robotII $(ROBOT_OBJS)

install:
	$(INSTALL) robotII $(bindir)

distclean: clean
	-rm -f Makefile
