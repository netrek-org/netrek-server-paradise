# Makefile.in for Paradise server src/common 3.1
# @configure_input@

@SET_MAKE@

bindir=@bindir@
datadir=@datadir@
exec_prefix=@exec_prefix@
libdir=@libdir@
libexecdir=@libexecdir@
mandir=@mandir@
prefix=@prefix@
srcdir=@srcdir@
top_srcdir=@top_srcdir@

AR=ar
ARFLAGS=cruv
RANLIB=@RANLIB@

CC=@CC@
CPP=@CPP@

CFLAGS=@CFLAGS@
CPPFLAGS=@CPPFLAGS@ -I../include @AUTH_DEFS@ @DEFS@
LDFLAGS=@LDFLAGS@
LIBS=@LIBS@ -lm

INSTALL=@INSTALL@
INSTALL_SCRIPT=@INSTALL_SCRIPT@
INSTALL_DATA=@INSTALL_DATA@

AUTH_OBJS=@AUTH_OBJS@
AUTH_PROG=@AUTH_PROG@
AUTH_PROG_OBJS=@AUTH_PROG_OBJS@

#####

all: $(AUTH_PROG) rsa.a

rsa.a: $(AUTH_OBJS)
	$(AR) $(ARFLAGS) $@ $(AUTH_OBJS)

$(AUTH_PROG): $(AUTH_PROG_OBJS)
	$(CC) $(CFLAGS) -o $(AUTH_PROG) $(AUTH_OBJS) $(LDFLAGS) $(LIBS)

clean:
	-rm -f rsa.a $(AUTH_OBJS) $(AUTH_PROG)

install:
	$(INSTALL) $(AUTH_PROG) $(prefix)/bin

distclean:
	-rm -f Makefile
