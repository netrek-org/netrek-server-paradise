# Makefile.in for Paradise server src/common 3.1
# @configure_input@

@SET_MAKE@

bindir=@bindir@
datadir=@datadir@
exec_prefix=@exec_prefix@
libdir=@libdir@
libexecdir=@libexecdir@
mandir=@mandir@
prefix=@prefix@
srcdir=@srcdir@
top_srcdir=@top_srcdir@

AR=ar
ARFLAGS=cruv
RANLIB=@RANLIB@

CC=@CC@
CPP=@CPP@

CFLAGS=@CFLAGS@
CPPFLAGS=@CPPFLAGS@ -I../../include
DEFS=@AUTH_DEFS@ @DEFS@
LDFLAGS=@LDFLAGS@
LIBS=@LIBS@ -lm

INSTALL=@INSTALL@
INSTALL_SCRIPT=@INSTALL_SCRIPT@
INSTALL_DATA=@INSTALL_DATA@

####

PLGEN_OBJS=pl_gen0.o \
           pl_gen1.o \
	   pl_gen2.o \
	   pl_gen3.o \
	   pl_gen4.o \
	   pl_gen5.o \
	   pl_gen6.o \
	   pl_gen7.o \
	   pl_gen8.o

all: pl_gen.a

pl_gen.a: $(PLGEN_OBJS)
	$(AR) $(ARFLAGS) $@ $(PLGEN_OBJS)
	$(RANLIB) $@

clean:
	-rm -f $(PLGEN_OBJS) pl_gen.a

# nothing to do for install
install:

distclean: clean
	-rm -f Makefile
